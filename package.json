{
	"name": "meme-registry",
	"version": "1.0.0",
	"description": "Registry of memes by @stephansama",
	"license": "MIT",
	"author": {
		"name": "Stephan Randle",
		"email": "stephanrandle@hotmail.com",
		"url": "https://stephansama.info"
	},
	"type": "module",
	"main": "registry.js",
	"scripts": {
		"build": "node ./registry.js",
		"dev": "node --watch ./registry.js"
	},
	"prettier": {
		"jsxSingleQuote": false,
		"plugins": [
			"prettier-plugin-jsdoc",
			"prettier-plugin-packagejson",
			"prettier-plugin-organize-imports"
		],
		"quoteProps": "consistent",
		"semi": true,
		"singleQuote": false,
		"useTabs": true
	},
	"devDependencies": {
		"prettier": "^3.6.2",
		"prettier-plugin-jsdoc": "^1.5.0",
		"prettier-plugin-organize-imports": "^4.3.0",
		"prettier-plugin-packagejson": "^2.5.19",
		"zod": "^4.1.12"
	},
	"packageManager": "pnpm@10.13.1"
}
